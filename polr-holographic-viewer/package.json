{
  "name": "polr-holographic-viewer",
  "version": "1.0.0",
  "description": "Holographic Construction Detail Viewer with Semantic Compression - R&D Prototype",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
